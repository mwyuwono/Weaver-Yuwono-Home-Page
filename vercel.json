{
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "github": {
    "silent": false,
    "autoAlias": true
  },
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/matt/:path*",
      "has": [
        {
          "type": "host",
          "value": "matt.weaver-yuwono.com"
        }
      ]
    }
  ]
}
